{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/telegram.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/email.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/telegram",
      "dest": "/api/telegram.ts"
    },
    {
      "src": "/api/email",
      "dest": "/api/email.ts"
    }
  ]
}
